{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"$lib": ["./src/frontend/src/lib"],
			"$lib/*": ["./src/frontend/src/lib/*"],
			"$declarations": ["./src/declarations"],
			"$declarations/*": ["./src/declarations/*"]
		},
		"types": ["node"]
	}
}
